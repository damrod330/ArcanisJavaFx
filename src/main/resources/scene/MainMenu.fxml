<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<HBox style="-fx-border-color: #aeafad; -fx-background-color: #ffffffbb;" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Text fill="#393737" strokeType="OUTSIDE" strokeWidth="0.0" text="ARCANIS">
         <font>
            <Font name="Constantia Bold" size="64.0" />
         </font>
      </Text>
      <Button fx:id="btnMainPage" mnemonicParsing="false" onMouseClicked="#btnMainPageClicked" prefHeight="64.0" prefWidth="124.0" text="Main " />
      <Button fx:id="btnQuestPage" mnemonicParsing="false" onMouseClicked="#btnQuestPageClicked" prefHeight="64.0" prefWidth="124.0" text="Quests" />
      <Button fx:id="btnLoginPage" mnemonicParsing="false" onMouseClicked="#btnLoginPageClicked" prefHeight="64.0" prefWidth="124.0" text="Login" />
   </children>
   <padding>
      <Insets bottom="24.0" left="24.0" right="24.0" top="24.0" />
   </padding>
</HBox>
